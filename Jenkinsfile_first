pipeline{
    agent none
    stages{
        stage("checkout code to machine"){
            agent any
            steps{
                echo "========executing A========"
                echo "checkout code from scm to localhost"
                git 'https://github.com/pypy-a/reader.git'
                echo "$BUILD_NUMBER"
                sh 'sed -i -e "s/1.0.0/1.0.$BUILD_NUMBER/" setup.py'
            }
        }
    }
}
